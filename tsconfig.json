{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"allowJs": false,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"baseUrl": ".",
		"paths": {
			"@basic": ["basic"],
			"@components": ["src/components"],
			"@components/*": ["src/components/*"],
			"@themes": ["src/themes"],
			"@themes/*": ["src/themes/*"],
			"@hooks/*": ["src/hooks/*"]
		}
	}
}
